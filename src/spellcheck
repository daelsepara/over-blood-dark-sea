#!/bin/bash
spell -n bloodsea.zil -b | grep -v -Ev "DESC|LIGHTBIT|CRLF|HLIGHT|FCLEAR|COND|LTABLE|PRECHOICE|FSET|PUTP|WORNBIT|WEARBIT|SETG|AUX|GETP|ALLMONEY|RTRUE|RFALSE|EMPHASIZE|READLINE|READBUF|LEXBUF|GETB|CONTBIT|OPENBIT|VOWELBIT|NARTICLEBIT|NDESCBIT|INTBL|PLURALBIT|PERSONBIT|TOUCHBIT|TAKEBIT|COLOR|CURSET|LOWCORE|WEAPONBIT|CACHEBIT|CURSEBIT|POISONBIT|DISEASEBIT|PRINTI|SCRH|PRINTC|PROPDEF|RELEASEID|IFID|NPC|XZIP|PRINTN|GIVEBAG|PRICELIST|DEFENSE|LASTFIX|ZIL|: SD$|: CT$|: OBJ$|: STR$" | grep -i -v -Ev "minilib|gamebook|defense|reroll|initialize|barque|lockpicks|mage|sorcerous|starspike|smogmaw|darkling|spindrift|gobbet|knucklebones|untended" | grep -i -v -Ev ": separa$|: tyrnai$|: maka$|: sig$|: lacuna$|: alvir$|: valmir$|: elnir$|: nagil$|: golnir$|: yellowport$|: dweomer$|: jaluda$|: damontir$|: cumberbatch$|: greymalkin$|: varkung$|: metriciens$|: amcha$|: pre$| ankon$|: konu$|: innis$|: nivram$|: maingauche$|: trefoille$|: sokara$|: sokaran$|: sokarans$|: badogor$|: cap'n$|: vervayens$|: uttaku$|: uttakin$|: sokar$|: marlock$|: braelak$" |uniq
