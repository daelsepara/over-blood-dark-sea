			<EMPHASIZE "You are not injured!">
			<CRLF>
			<TELL "Do you want to drop this instead?">
			<COND (<YES?> <REMOVE-ITEM ,POTION-OF-HEALING "dropped" T T>)>
		)>
	)(ELSE
		<EMPHASIZE "You do not have a potion of healing?">
	)>
